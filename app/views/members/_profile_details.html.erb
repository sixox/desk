<div class="card mb-4">
	<div class="card-body">
		<div class="row">
			<div class="col-sm-3">
				<p class="mb-0">Full Name</p>
			</div>
			<div class="col-sm-9">
				<p class="text-muted mb-0"><%= user.name %></p>
			</div>
		</div>
		<hr>
		<div class="row">
			<div class="col-sm-3">
				<p class="mb-0">Email</p>
			</div>
			<div class="col-sm-9">
				<p class="text-muted mb-0"><%= user.email %></p>
			</div>
		</div>
		<hr>
		<div class="row">
			<div class="col-sm-3">
				<p class="mb-0">Phone</p>
			</div>
			<div class="col-sm-9">
				<p class="text-muted mb-0"><%= user.office_num %></p>
			</div>
		</div>
		<hr>
		<div class="row">
			<div class="col-sm-3">
				<p class="mb-0">Mobile</p>
			</div>
			<div class="col-sm-9">
				<p class="text-muted mb-0"><%= user.mobile_num %></p>
			</div>
		</div>
		<hr>
		<div class="row">
			<div class="col-sm-3">
				<p class="mb-0">Address</p>
			</div>
			<div class="col-sm-9">
				<p class="text-muted mb-0">Bay Area, San Francisco, CA</p>
			</div>
		</div>
	</div>
</div>